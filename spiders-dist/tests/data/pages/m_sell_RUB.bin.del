  function getTagList(ids){
    var tags = [];
    $.each(ids, function(i, el){
      $.each(mfSelectJSON, function(_i, _el){
        if(_i === el){
          tags = tags.concat( _el.tags );
        }
      });
    });
    return tags;
  }


  function checkForEmptyResults(){
    $.each($('.au-deals-list'),function(i,el){
      if ($(el).find('.au-deal:visible').length > 0) {
        $(el).find('.au-deal-empty').remove();
      } else if($(el).find('.au-deal-empty').length === 0){
        $(el).append('<div class="au-deal-empty"><span>Нет объявлений, соответствующих данным параметрам</span></div>')
      }
    });
  }



  function scanDeals(id, keyWord,dealDiv){
    $.each(dealsList, function(i,el){
      if(el.text.indexOf(keyWord.toLowerCase())>-1){
        $(el.deal).addClass(id);
      }
    });
  }
  function applyFilterJSON(){
    $.each(mfSelectJSON,function(id,el){
      $.each(el.tags,function(_i,keyWord){
        scanDeals(id,keyWord,el.deal);
      })
    });
  };
  function getDeals(){
    var arr = [];
    $.each($('.au-deal'),function(i,el){
      var txt = $(el).find('.au-deal-msg').text();
      arr.push({deal:el,text:txt.toLowerCase()})
    });
    return arr;
  };
  var dealsList = getDeals();
  applyFilterJSON();

  function filterDeals(ids){
    if(ids.length>0){
      $('.au-deal').not('.au-premium-deal').addClass('mfz-hidden');
      $.each(ids,function(i,el){
        $('.au-deal.'+el.id).not('.au-premium-deal').removeClass('mfz-hidden');
      });
    }else{
      $('.au-deal').not('.au-premium-deal').removeClass('mfz-hidden');
    }
    checkForEmptyResults();
  }

  function createMfSelectTag(wgtBody, ids){
    $(wgtBody).find('.mfSelect-bottom-tags').html('');
    $.each(ids,function(i,el){
      var tag = $('<a></a>');
      tag.addClass(el.id).text(el.txt);
      $(wgtBody).find('.mfSelect-bottom-tags').append(tag);
    });
  }

  function insertSelectedLabels(self, ids){
    var selectedLabels = '';
    $.each(ids, function(i,el){
      var comma = (i!==0)?', ':'';
      selectedLabels += comma + el.txt;
    });
    selectedLabels = (selectedLabels.length>17) ? selectedLabels.slice(0,17)+'...' : selectedLabels;
    console.log(self);
    if(selectedLabels.length === 0){
      selectedLabels = self.itemsCollection[0].innerText;
      $(self.itemsCollection[0]).find('input').prop('checked',true);
      $(self.itemsCollection[0]).addClass('active');
    }
    $(self).val(selectedLabels);
  }

  $('.js-district-filter').mfSelect({
      wgtBody:'.js-district-filter-body',
      wgtCustomWidth:205,
      triangle: false,
      offsetXY: [-25,25],
      closeOnSelect: false,
      multiSelect: true,
      triangle: true,
      bottomLink: true,
      onItemClick: function(e, self){
        // target fix
        var target = (e.currentTarget) ? e.currentTarget : e.srcElement;
        var ids = [];
        if($('.js-user-type').attr('data-user-type') !== 'pro'){
          if(typeof doProPromoTips !== 'undefined'){
            doProPromoTips({data:{promo:'promoFiltersUsedOrScroll',targ:'body'}});
          }
        }
        // collect checked checkboxes to 'ids' array
        $.each($(self.wgtWrap).find('.mfSelect-checkbox[type=checkbox]:checked'),function(i,el){
          var elID = $(el).val(), labelText = $(el).parent().text();
          ids.push({ id:elID, txt:labelText });
        });
        // if array isn't empty
        if(ids.length > 0){
          // if all districts was selected uncheck all other checkboxes and return false
          if($(target).find('input').val() === 'dfMfs0'){
            $(self.wgtWrap).find('.mfSelect-list-item:not(:nth(0))').removeClass('active');
            $(self.wgtWrap).find('.mfSelect-checkbox[type=checkbox]:not(:nth(0))').prop('checked',false);
            // remove all other values from generated array
            var allDistricts = ids[0];
            ids.length = 0;
            ids.push(allDistricts);
          }else{
            // allways uncheck 'all districs' if any other checkbox was checked
            $(self.wgtWrap).find('.mfSelect-checkbox[type=checkbox]:nth(0)').prop('checked',false);
            $(self.wgtWrap).find('.mfSelect-list-item:nth(0)').removeClass('active');
            // remove 'all districs' from generated array
            if(ids[0].id === 'dfMfs0'){
              ids.shift();
            }
          }
          // if(ids[0].id === 'dfMfs0'){
            // $(self.wgtWrap).find('.mfSelect-checkbox[type=checkbox]:not(:nth(0))').prop('checked',false);
            // return false;
          // }
        }
        if($(target).find('input').is(':checked')){
          sendCode('filter' , $(target).text());
        }
        // pass them to filter func
        filterDeals(ids);
        // draw tags
        insertSelectedLabels(self, ids);
      },
      onWgtClose: function(self){ }
    });

    $(document).ready(function() {
      var $offerThanks = $('#offerThanks');
      if ($offerThanks.length){ $offerThanks.modal(); }
    });
  </script>



